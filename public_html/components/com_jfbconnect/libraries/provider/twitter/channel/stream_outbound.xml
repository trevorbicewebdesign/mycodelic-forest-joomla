<?xml version="1.0" encoding="utf-8"?>
<form>
    <fields name="attribs">
        <fieldset name="attribs">
            <field name="user_id"
                   type="channeluserlist"
                   label="COM_JFBCONNECT_CHANNEL_USER_ID_LABEL"
                   description="COM_JFBCONNECT_CHANNEL_USER_ID_DESCRIPTION"
                   onchange="jfbcAdmin.channels.outbound.onuserchange(this.value)"
                   default=""
                   labelclass="config_setting"
                   class="twitter"
                    />
            <field name="include_rts"
                   type="radio"
                   label="COM_JFBCONNECT_CHANNEL_INCLUDE_RTS_LABEL"
                   description="COM_JFBCONNECT_CHANNEL_INCLUDE_RTS_DESCRIPTION"
                   default="1"
                   labelclass="config_setting"
                   class="btn-group"
            >
                <option value="0">JNO</option>
                <option value="1">JYES</option>
            </field>
        </fieldset>
    </fields>
</form>
