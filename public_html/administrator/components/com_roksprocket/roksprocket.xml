<?xml version="1.0" encoding="utf-8"?>
<extension type="component" version="2.5" method="upgrade">
    <name>RokSprocket</name>
    <creationDate>April 23, 2020</creationDate>
    <author>RocketTheme, LLC</author>
    <authorEmail>support@rockettheme.com</authorEmail>
    <authorUrl>http://www.rockettheme.com</authorUrl>
    <copyright>(C) 2005 - 2020 RocketTheme, LLC. All rights reserved.</copyright>
    <license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
    <version>2.1.28</version>
    <description>RokSprocket</description>

    <files folder="site">
        <folder>assets</folder>
        <folder>fields</folder>
        <folder>language</folder>
        <folder>layouts</folder>
        <folder>lib</folder>
        <folder>tables</folder>
        <filename>container.xml</filename>
        <filename>controller.php</filename>
        <filename>module_config.xml</filename>
        <filename>roksprocket.php</filename>
        <filename>MD5SUMS</filename>
    </files>

    <scriptfile>roksprocket_install.script.php</scriptfile>

    <install>
        <sql>
            <file driver="mysql" charset="utf8">install/install.mysql.utf8.sql</file>
        </sql>
    </install>
    <uninstall>
        <sql>
            <file driver="mysql" charset="utf8">install/uninstall.mysql.utf8.sql</file>
        </sql>
    </uninstall>

    <administration>
        <files folder="admin">
            <folder>assets</folder>
            <folder>controllers</folder>
            <folder>helpers</folder>
            <folder>install</folder>
            <folder>language</folder>
            <folder>models</folder>
            <folder>templates</folder>
            <folder>views</folder>
            <filename>controller.php</filename>
            <filename>roksprocket.php</filename>
            <filename>MD5SUMS</filename>
        </files>
    </administration>
</extension>
